<component name="wunderlist-folder" type="synapse/template">
    <subComponents>
        <component name="createFolder">
            <file>createFolder.xml</file>
            <description>Create a new folder in WunderList account using the provided Parameters.</description>
        </component>
        <component name="listFolders">
            <file>listFolders.xml</file>
            <description>Retrieve a List of folders from WunderList account.</description>
        </component>
        <component name="getFolder">
            <file>getFolder.xml</file>
            <description>Retrieve details of a folder identified by the provided folder ID.</description>
        </component>
        <component name="updateFolder">
            <file>updateFolder.xml</file>
            <description>Update details of the folder identified by the provided folder ID.</description>
        </component>
    </subComponents>
</component>
